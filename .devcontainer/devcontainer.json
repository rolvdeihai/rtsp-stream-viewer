{
  "name": "rtsp-stream-viewer",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspaces/rtsp-stream-viewer",
  "forwardPorts": [7860, 3000],
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "extensions": [
    "ms-python.python"
  ],
  "postCreateCommand": "echo 'Daphne backend and frontend will auto-start on container start.'"
}
